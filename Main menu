from tkinter import *  
  
window = Tk()  
window.minsize(800,800)
window.title('IP Project 2020')

def home():  
    print("xyz")  

def advisory():  
    print("xyz") 
    
def dailyDataAnalysis():  
    print("xyz")  

def districtData():  
    print("xyz")  

def modelledSpread():  
    print("xyz")  

def survey():  
    print("xyz")  

  
# create a toplevel menu  
menubar = Menu(window)  
menubar.add_command(label="Home", command=home) 
menubar.add_command(label="Advisory", command=advisory) 
menubar.add_command(label="Daily Data Analysis", command=dailyDataAnalysis)  
menubar.add_command(label="District Data Analysis", command=districtData)  
menubar.add_command(label="Covid-19 Spread Model", command=modelledSpread)  
menubar.add_command(label="Survey", command=survey)  
menubar.add_command(label="Quit", command=window.destroy)  
  
# display the menu  
window.config(menu=menubar)  
  
window.mainloop()  
